<match notification.**>
  type copy

  <store>
    type buffered_slack
    api_key <%= @api_token %>
    team kazu634
    channel %23ops
    username kazu-chan
    color danger
    icon_emoji :ghost:
    buffer_path /var/log/td-agent/buffer/notification_buffer*.buffer
    flush_interval 60s
  </store>

  <store>
    type file
    path /tmp/notificaiton.log
    buffer_type file
    buffer_path /var/log/td-agent/buffer/notification*.buffer
  </store>
</match>
